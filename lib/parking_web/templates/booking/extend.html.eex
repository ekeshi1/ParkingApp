<%= form_for @changeset, Routes.booking_path(@conn, :extend), fn f -> %>
    <h2>Extend Parking</h2>

  <div class="form-group">
  </div>
  <%= hidden_input f, :id%>
  </div>

  <div class="form-group">
    <%= label f, :end_time %>
    <%= time_select f, :end_time , class: "form-control"%>
    <%= error_tag f, :end_time %>
  </div>

  <div class="form-group">
    <%= submit "Submit", id: "submit_button", class: "btn btn-primary" %>
  </div>

<% end %>
